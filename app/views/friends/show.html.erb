


<%# <%= render @friend %>
<div class="jumbotron">
  <h1 class="display-4">
    <%= @friend.first_name %>
     <%= @friend.last_name %>
    </h1>
  <hr class="my-4">
  <p> <%= @friend.phone %>  || <%= @friend.twitter %></p>
  <p class="lead">
      <%= link_to "Edit this friend", edit_friend_path(@friend), class:"btn btn-secondary"%> |
  <%= link_to "Back to friends", friends_path,class:"btn btn-dark" %>
  </p>
</div>






